<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.crewmanagement">

    "<uses-permission android:name="android.permission.INTERNET" />"

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Navigation" />
        <activity android:name=".ChangePassword" />
        <activity android:name=".NewMember" />
        <activity android:name=".ViewScreen"/>
        <activity android:name=".Administrator"
            android:label="Administrator"
            android:parentActivityName=".Navigation">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".Navigation"></meta-data>
        </activity>

        <activity android:name=".MembersScreen"
            android:label="MembersScreen"
            android:parentActivityName=".Navigation">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".Navigation"></meta-data>
        </activity>
        <activity android:name=".ProgressScreen"
            android:label="ProgressScreen"
            android:parentActivityName=".Navigation">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".Navigation"></meta-data>
        </activity>
        <activity android:name=".Profile"
            android:label="Profile"
            android:parentActivityName=".Navigation">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".Navigation"></meta-data>
        </activity>
        <activity android:name=".NewsFeed"
            android:label="NewsFeed"
            android:parentActivityName=".Navigation">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".Navigation"></meta-data>
        </activity>
        <activity android:name=".JobAssignment"
            android:label="Job Assignment">
        </activity>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>